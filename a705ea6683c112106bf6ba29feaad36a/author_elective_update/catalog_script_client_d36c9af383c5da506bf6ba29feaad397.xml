        <script><![CDATA[function onChange() {
    var expProfissional = g_form.getValue('experiencias_profissionais');
    var habilComp = g_form.getValue('habilidades_e_competencias');
	var formAcadem = g_form.getValue('formacao_academica');
	var obs = g_form.getValue('observacoes');

    if (expProfissional) {
        // Remove as tags <p>
        var expSemTags = expProfissional.replace(/<\/?p>/g, '');
        g_form.setValue('experiencias_profissionais', expSemTags);
		
        var habilSemTags = habilComp.replace(/<\/?p>/g, '');
        g_form.setValue('habilidades_e_competencias', habilSemTags);
		
        var formSemTags = formAcadem.replace(/<\/?p>/g, '');
        g_form.setValue('formacao_academica', formSemTags);

		var obsSemTags = obs.replace(/<\/?p>/g, '');
        g_form.setValue('observacoes', obsSemTags);
    }
}
]]></script>
